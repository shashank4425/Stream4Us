{
  "expo": {
    "name": "Stream4Us",
    "slug": "Stream4Us",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/stream4us/logo/stream4us_logo.png",
    "scheme": "com.example.streamus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0D0E10",
        "foregroundImage": "./assets/images/stream4us/logo/stream4us_logo.png",
        "backgroundImage": "./assets/images/stream4us/logo/stream4us_logo.png",
        "monochromeImage": "./assets/images/stream4us/logo/stream4us_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.shashank4425.Stream4Us"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/stream4us/logo/stream4us_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "cover",
          "backgroundColor": "#0D0E10",
          "dark": {
            "backgroundColor": "#0D0E10"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
