{
  "expo": {
    "name": "Stream4Us",
    "slug": "Stream4Us",
    "version": "1.1.4",
    "orientation": "DEFAULT",
    "icon": "./assets/images/stream4us/logo/stream4us_logo.png",
    "scheme": "com.example.streamus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0D0E10",
        "foregroundImage": "./assets/images/stream4us/logo/stream4us_logo.png",
        "backgroundImage": "./assets/images/stream4us/logo/stream4us_logo.png",
        "monochromeImage": "./assets/images/stream4us/logo/stream4us_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.shashank4425.Stream4Us"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/stream4us/logo/stream4us_logo.png"
    },
    "plugins": [
      "expo-router",
      [
      "expo-navigation-bar",
       {
        "behavior":"overlay-swipe",
        "visibility": "hidden"
       }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/stream4us/logo/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#0D0E10",
          "dark": {
            "backgroundColor": "#0D0E10"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b43ce736-be96-4ed0-a7fa-84326db8e428"
      }
    }
  }
}
